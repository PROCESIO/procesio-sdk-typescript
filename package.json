{
  "name": "@procesio/procesio-sdk-typescript",
  "version": "1.0.0",
  "description": "SDK for running Procesio processes.",
  "main": "build/index.js",
  "types": "build/types/index.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.demo.js",
    "build": "npx typedoc --out docs src/lib/index.ts && webpack && tsc",
    "build:demo": "webpack --config webpack.config.demo.js",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest --silent",
    "coverage": "npm run test -- --coverage",
    "prepare": "npm run build",
    "trypublish": "npm publish || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PROCESIO/procesio-sdk-typesscript"
  },
  "author": "PROCESIO (https://github.com/PROCESIO)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PROCESIO/procesio-sdk-typesscript/issues"
  },
  "homepage": "",
  "keywords": [
    "procesio-sdk",
    "sdk",
    "es6"
  ],
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/plugin-transform-typescript": "^7.15.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.4",
    "@babel/runtime": "^7.15.4",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.0",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.1.0",
    "mini-css-extract-plugin": "^2.2.2",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.3.2",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.2.3",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "4.1.1",
    "typedoc": "^0.22.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/lib/index.js",
      "!src/demo/index.js",
      "!src/utils/**",
      "!**/tests/**"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
